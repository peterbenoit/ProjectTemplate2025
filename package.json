{
  "name": "[PROJECT_NAME]",
  "version": "1.0.0",
  "description": "[PROJECT_DESCRIPTION]",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run update-sitemap && vite build",
    "preview": "vite preview",
    "start": "npm run dev",
    "update-sitemap": "node scripts/update-sitemap.js",
    "prebuild": "npm run update-sitemap",
    "lint": "eslint . --ext .js,.vue",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "prepare": "command -v husky >/dev/null && husky install || echo 'Husky not installed'",
    "setup": "node scripts/project-setup.js",
    "test:template": "node scripts/test-template.js"
  },
  "keywords": "[PROJECT_KEYWORDS]",
  "author": "[PROJECT_AUTHOR]",
  "license": "MIT",
  "dependencies": {
    "pinia": "^2.1.0",
    "remixicon": "^4.6.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.20.0",
    "husky": "^9.0.10",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "terser": "^5.40.0",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
